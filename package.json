{
    "name": "webdav",
    "displayName": "WebDAV Upload",
    "description": "Uploads local files to a remote host using WebDAV",
    "version": "0.0.7",
    "publisher": "jorithvdheuvel",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.webdavUpload",
                "title": "WebDAV: Upload"
            },
            {
                "command": "extension.webdavCompare",
                "title": "WebDAV: Compare"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "homepage": "https://github.com/jorith88/vscode-webdav/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/jorith88/vscode-webdav/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jorith88/vscode-webdav.git"
    },
    "devDependencies": {
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.12.10",
        "eslint": "^5.9.0",
        "mocha": "^5.2.0"
    },
    "dependencies": {
        "find-config": "^1.0.0",
        "path": "^0.12.7",
        "q": "^1.5.1",
        "tmp": "0.0.33",
        "typescript": "^3.4.1",
        "underscore": "^1.9.1",
        "vscode": "^1.1.22",
        "webdav-fs": "^1.12.0"
    }
}
